{
  "name": "gong-be",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint server lib routes",
    "lint:fix": "npm run lint -- --fix",
    "start": "NODE_ENV=development babel-node server",
    "clean": "rm -rf build && mkdir build",
    "copy-modules": "cp -rf node_modules ./build",
    "copy-dist": "cp -rf dist ./build",
    "copy-assets": "cp -rf assets ./build",
    "copy": "npm run copy-modules && npm run copy-dist && npm run copy-assets",
    "build-server": "babel -s -d ./build ./ --ignore \"dist\",\"test\",\"jest\",\"node_modules\"",
    "build": "npm run clean && npm run build-server && npm run copy",
    "start-deployed": "NODE_ENV=production node ./build/server",
    "start-nodemon": "nodemon --exec babel-node server"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "FT245RL": "doron-mil/node-FT245RL",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "config": "^2.0.1",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "helmet": "^3.13.0",
    "json-class-converter": "^1.0.1",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "node-schedule": "^1.3.1",
    "play-sound": "^1.1.3",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.3.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.5.1",
    "jest": "^24.8.0",
    "nodemon": "^1.18.3",
    "sinon": "^7.3.2",
    "supertest": "^3.1.0"
  },
  "jest": {
    "testMatch": [
      "**/jest/**/*.test.js"
    ]
  }
}
